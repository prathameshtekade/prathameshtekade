package Stack;
import java.util.Scanner;

public class StackDemo {

	public static void main(String[] args) {
		long n,count=0;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a Number");
		 n=sc.nextLong();
		
		 while(n>0)
 {
	n=n/10;
	count++;
 }
		 System.out.println("Total Numbers Are = "+count);

	} 
}
